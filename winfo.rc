        BLOCK "080904b0"
        BEGIN
            VALUE "CompanyName", "https://github.com/kociumba/winfo"
            VALUE "FileDescription", "winfo"
            VALUE "FileVersion", "0.0.0.0"
            VALUE "InternalName", "winfo.exe"
            VALUE "LegalCopyright", "Copyright (c) 2025"
            VALUE "OriginalFilename", "winfo.exe"
            VALUE "ProductName", "winfo - display info about windows"
            VALUE "ProductVersion", "0.0.0.0"
            VALUE "Comments", "winfo is a tool to display information about windows, like their class name, extended styles and much more"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x809, 1200
    END
END

IDI_ICON1   ICON    "assets\\winfo.ico"